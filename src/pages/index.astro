---
import BaseHead from "@/components/BaseHead.astro";
import Header from "@/components/Header.astro";
import Footer from "@/components/Footer.astro";
import { SITE_TITLE, SITE_DESCRIPTION } from "../consts";
import Hero from "@/components/Hero.astro";
import WhatIDo from "@/components/WhatIDo.astro";

---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <body class="p-0 m-0 max-w-screen">
    <Header />
    <main class="container mx-auto md:px-10 min-h-screen bg-[url(/src/assets/bglol.png)] bg-[auto_200px] bg-no-repeat bg-[100%_78%]">
      <a id="yahooBtn" class="cursor-pointer">
        <iframe width="100" height="100" src="https://www.myinstants.com/instant/yahoo-yodel-sound-effect-56481/embed/" frameborder="0" scrolling="no"></iframe></marquee>
      </a>
    </main>
  </body>
</html>
